{
  "plugins": ["@prettier/plugin-php", "prettier-plugin-css-order"],
  "semi": false,
  "tabWidth": 2,
  "useTabs": false,
  "singleQuote": true,
  "overrides": [
    {
      "files": ["*.php", "*.kirby"],
      "options": {
        "parser": "php",
        "phpVersion": "8.1",
        "braceStyle": "1tbs"
      }
    }
  ]
}
